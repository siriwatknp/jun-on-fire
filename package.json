{
  "name": "jun-on-fire",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4416",
    "dev:prod": "NEXT_PUBLIC_FIREBASE_ENV=production next dev -p 4416",
    "run:prod": "NEXT_PUBLIC_FIREBASE_EMULATOR=false NEXT_PUBLIC_FIREBASE_ENV=production next dev -p 4416",
    "build": "next build",
    "build:functions": "cd functions && npm run build",
    "start": "next start",
    "lint": "next lint",
    "emulators": "cd functions && npm run build:watch & cd .. & firebase emulators:start --import .firebase-local-data --export-on-exit",
    "dev:all": "concurrently \"npm run dev\" \"npm run emulators\"",
    "deploy:functions": "cd functions && npm run build && cd .. && firebase deploy --only functions",
    "test:deploy": "cd functions && npm run build && echo \"âœ… Functions build successful and ready for deployment\"",
    "populate": "NODE_ENV=development NEXT_PUBLIC_FIREBASE_EMULATOR=false ts-node --project tsconfig-script.json scripts/populate-mock-data.ts"
  },
  "dependencies": {
